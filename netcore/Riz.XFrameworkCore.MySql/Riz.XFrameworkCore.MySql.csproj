    <Compile Include="..\..\net45\Riz.XFramework.MySql\Data\SqlClient\MySqlMethodCallExressionVisitor.cs" Link="Data\SqlClient\MySqlMethodCallExressionVisitor.cs" />
    <Compile Include="..\..\net45\Riz.XFramework.MySql\Data\SqlClient\MySqlTranslateContext.cs" Link="Data\SqlClient\MySqlTranslateContext.cs" />
